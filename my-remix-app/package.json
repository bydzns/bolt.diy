{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix .",
    "format": "prettier --write . --ignore-path .gitignore --ignore-unknown",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "migrate": "node-pg-migrate -m db/migrations --config pg-migrate-config.js",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "migrate:create": "node-pg-migrate -m db/migrations --config pg-migrate-config.js create"
  },
  "dependencies": {
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "bcryptjs": "^3.0.2",
    "isbot": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.25.48"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-pg-migrate": "^8.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
